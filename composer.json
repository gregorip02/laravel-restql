{
  "name": "gregorip02/restql",
  "description": "A data resolution package for Laravel",
  "type": "library",
  "keywords": ["restql", "eloquent", "laravel", "graphql", "drp"],
  "url": "https://github.com/gregorip02/restql",
  "license": "LGPL-3.0",
  "authors": [
    {
      "name": "Gregori Pi√±eres",
      "email": "gregori.pineres02@gmail.com",
      "homepage": "https://github.com/gregorip02"
    }
  ],
  "support": {
    "issues": "https://github.com/gregorip02/restql/issues",
    "source": "https://github.com/gregorip02/restql"
  },
  "require": {
    "php": "^7.1",
    "illuminate/http": "^5.8|^6.0|^7.0",
    "illuminate/database": "^5.8|^6.0|^7.0",
    "illuminate/config": "^5.8|^6.0|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "orchestra/testbench": "^3.8|^4|^5"
  },
  "autoload": {
    "psr-4": {
      "Restql\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Restql\\Providers\\RestqlServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
