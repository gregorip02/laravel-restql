{
  "name": "gregorip02/restql",
  "description": "A data resolution package for Laravel",
  "type": "library",
  "keywords": ["restql", "eloquent", "laravel", "graphql", "drp"],
  "url": "https://github.com/gregorip02/restql",
  "license": "LGPL-3.0",
  "authors": [
    {
      "name": "Gregori Pi√±eres",
      "email": "gregori.pineres02@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/gregorip02/restql/issues",
    "source": "https://github.com/gregorip02/restql"
  },
  "require": {
    "php": "^7.1",
    "illuminate/http": "^7.0",
    "illuminate/support": "^7.0",
    "illuminate/database": "^7.0",
    "illuminate/container": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "autoload": {
    "psr-4": {
      "Restql\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "test/laravel/app/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Restql\\RestqlServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
